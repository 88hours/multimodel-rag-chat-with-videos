[{"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_0.jpg", "transcript": " Black holes are among the most fascinating objects in our universe, and also the most", "video_segment_id": 0, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 4380.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_1.jpg", "transcript": " mysterious.", "video_segment_id": 1, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 11140.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_2.jpg", "transcript": " A black hole is a region in space where the force of gravity is so strong, not even light,", "video_segment_id": 2, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 16280.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_3.jpg", "transcript": " the fastest known entity in our universe can escape.", "video_segment_id": 3, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 21120.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_4.jpg", "transcript": " The boundary of a black hole is called the event horizon, a point of no return beyond", "video_segment_id": 4, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 25940.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_5.jpg", "transcript": " which we truly cannot see.", "video_segment_id": 5, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 30260.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_6.jpg", "transcript": " When something crosses the event horizon, it collapses into the black hole's singularity,", "video_segment_id": 6, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 34440.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_7.jpg", "transcript": " an infinitely small, infinitely dense point where space, time, and the laws of physics", "video_segment_id": 7, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 39720.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_8.jpg", "transcript": " no longer apply.", "video_segment_id": 8, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 44300.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_9.jpg", "transcript": " Scientists have theorized several different types of black holes, with stellar and supermassive", "video_segment_id": 9, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 48800.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_10.jpg", "transcript": " black holes being the most common.", "video_segment_id": 10, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 52840.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_11.jpg", "transcript": " Stolar black holes form when massive stars die and collapse.", "video_segment_id": 11, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 56460.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_12.jpg", "transcript": " They're roughly 10 to 20 times the mass of our sun, and scattered throughout the universe.", "video_segment_id": 12, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 61860.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_13.jpg", "transcript": " There could be millions of these stellar black holes in the Milky Way alone.", "video_segment_id": 13, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 68060.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_14.jpg", "transcript": " Supermassive black holes are giants by comparison, measuring millions, even billions of times", "video_segment_id": 14, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 73740.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_15.jpg", "transcript": " more massive than our sun.", "video_segment_id": 15, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 77940.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_16.jpg", "transcript": " Scientists can only guess how they form, but we do know they exist at the center of just", "video_segment_id": 16, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 81620.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_17.jpg", "transcript": " about every large galaxy, including our own.", "video_segment_id": 17, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 86360.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_18.jpg", "transcript": " Sagittarius A, the supermassive black hole at the center of the Milky Way, has a mass", "video_segment_id": 18, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 91340.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_19.jpg", "transcript": " of roughly four million suns, and has a diameter about the distance between the Earth and our", "video_segment_id": 19, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 96560.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_20.jpg", "transcript": " sun.", "video_segment_id": 20, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 100660.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_21.jpg", "transcript": " Because black holes are invisible, the only way for scientists to detect and study them", "video_segment_id": 21, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 104320.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_22.jpg", "transcript": " is to observe their effect on nearby matter.", "video_segment_id": 22, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 108360.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_23.jpg", "transcript": " This includes accretion disks, a disk of particles that form when gases and dust fall toward a", "video_segment_id": 23, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 112700.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_24.jpg", "transcript": " black hole, and quasars, jets of particles that blast out of supermassive black holes.", "video_segment_id": 24, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 119640.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_25.jpg", "transcript": " Black holes remained largely unknown until the 20th century.", "video_segment_id": 25, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 126320.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_26.jpg", "transcript": " In 1916, using Einstein's General Theory of Relativity, a German physicist named Karl", "video_segment_id": 26, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 131780.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_27.jpg", "transcript": " Schwartzschild calculated that any mass could become a black hole if it were compressed tightly", "video_segment_id": 27, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 137560.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_28.jpg", "transcript": " enough.", "video_segment_id": 28, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 141460.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_29.jpg", "transcript": " But it wasn't until 1971 when theory became reality.", "video_segment_id": 29, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 145060.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_30.jpg", "transcript": " Astronomers, studying the constellation Cygnus, discovered the first black hole.", "video_segment_id": 30, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 150740.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_31.jpg", "transcript": " An untold number of black holes are scattered throughout the universe, constantly warping", "video_segment_id": 31, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 156720.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_32.jpg", "transcript": " space and time, altering entire galaxies, and endlessly inspiring both scientists and", "video_segment_id": 32, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 162520.0}, {"extracted_frame_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/extracted_frame/frame_33.jpg", "transcript": " our collective imagination.", "video_segment_id": 33, "video_path": "./shared_data/videos/yt_video/blackholes101nationalgeographic/blackholes101nationalgeographic.mp4", "mid_time_ms": 166360.0}]